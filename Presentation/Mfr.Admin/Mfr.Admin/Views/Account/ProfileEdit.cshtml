@using System.Web.Optimization
@{
    Layout = "~/Views/Shared/_Main.cshtml";
}
@model Mfr.Admin.Models.Account.RegisterViewModel

<section id="container" class="">
    <!--main content start-->
    <section id="main-content">
        <section class="wrapper">
            <!-- page start-->
            <div class="row">
                <aside class="profile-info col-lg-9">

                    <section class="panel">
                        <div class="bio-graph-heading">
                            <h2>@ViewBag.Title</h2>
                        </div>

                        <div class="panel-body bio-graph-info">

                        </div>
                    </section>
                </aside>
            </div>
        </section>
    </section>
</section>

@section contents
{
    <link rel="stylesheet" type="text/css" href="~/Content/assets/bootstrap-datepicker/css/datepicker.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/assets/bootstrap-colorpicker/css/colorpicker.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/assets/bootstrap-daterangepicker/daterangepicker.css" />
}

@section scripts
{
    @Scripts.Render("~/bundles/jqueryval")

    <!--custom switch-->
    <script src="~/Scripts/bootstrap-switch.js"></script>
    <!--custom tagsinput-->
    <script src="~/Scripts/jquery.tagsinput.js"></script>
    <!--custom checkbox & radio-->
    <script type="text/javascript" src="~/Scripts/ga.js"></script>

    <script type="text/javascript" src="~/Content/assets/bootstrap-datepicker/js/bootstrap-datepicker.js"></script>
    <script type="text/javascript" src="~/Content/assets/bootstrap-daterangepicker/date.js"></script>
    <script type="text/javascript" src="~/Content/assets/bootstrap-daterangepicker/daterangepicker.js"></script>
    <script type="text/javascript" src="~/Content/assets/bootstrap-colorpicker/js/bootstrap-colorpicker.js"></script>


    <!--common script for all pages-->
    <script src="~/Scripts/common-scripts.js"></script>

    <!--script for this page-->
    <script src="~/Scripts/form-component.js"></script>
}


